{
	"name": "qkmake",
	"version": "1.0.0",
	"description": "A GUI typesetting display engine and cross platform GUI application development framework based on NodeJS/OpenGL",
	"main": "server.js",
	"types": "server",
	"author": {
		"name": "blue.chu",
		"email": "louistru@hotmail.com",
		"url": "http://quarks.cc/"
	},
	"bin": {
		"qkmake": "shell.js"
	},
	"scripts": {
		"cp": "mkdir -p out/qkmake && cp -rf LICENSE *.keys *.md marked export package.json out/qkmake && if [ ! -d out/qkmake/gyp ]; then ln -s ../../gyp out/qkmake; fi ",
		"build": "npm run cp && tsc"
	},
	"keywords": [
		"jsx",
		"javascript",
		"ecmascript",
		"html5",
		"css",
		"css3",
		"qt",
		"android",
		"ios",
		"appletv",
		"gui",
		"ui",
		"opengl",
		"react",
		"native",
		"es6",
		"es7",
		"es2017"
	],
	"dependencies": {
		"marked": "~0.3.6",
		"uglify-es": "~3.0.28",
		"suark": "~0.1.0",
		"typescript": "~5.4.5"
	},
	"devDependencies": {
		"@types/node": "~14.14.22"
	},
	"license": "BSD-2-Clause",
	"bugs": {
		"url": "https://github.com/louis-tru/quark/issues",
		"email": "louistru@hotmail.com"
	},
	"homepage": "http://quarks.cc/"
}
